<%= simple_form_for @projecct, :html => { :class => 'form-horizontal' , :multipart => true } do |f| %>

  <%= f.input :name %><br/>
  <%= f.input :owner %><br/>
  <%#= f.input :released_date %>
  <%= f.input :Description, input_html: { :rows => 4 , :cols => 20 } %><br/>  
  <%= f.input :subdomain %>


  <div class="form-actions">
    <%= f.button :submit, :class => 'btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                projeccts_path, :class => 'btn' %>
  </div>
<% end %>
